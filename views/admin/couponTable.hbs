                                            <th scope="col">Expiry Date</th>
                                            <th scope="col">Edit Coupon</th>
                                            <th scope="col">Delete Coupon</th>
                                        </tr>
                                    </thead> 

                                    <tbody>
                                     {{#each couponData}}
                                        <tr>
                                            <th scope="row">{{inc @index}}</th>
                                            <td>{{this.couponName}}</td>
                                            <td>{{this.couponCode}}</td>
                                            <td>{{this.discountAmount}}</td>
                                            <td>{{this.minAmount}}</td>
                                            <td>{{formatString this.expiryDate}}</td>
                                            <td><a href="/admin/editCoupon/{{this._id}}"><i class="bi bi-pencil-square"></i></a></td>
                                            <td><a href="/admin/deleteCoupon/{{this._id}}"><i class="bi bi-trash" onclick="return confirm('Are you sure to delete {{this.couponName}}?')"></i></a></td>
                                            
